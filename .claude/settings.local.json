      "Bash(cargo clippy:*)",
      "Bash(cargo search:*)",
      "Bash(cargo info:*)",
      "Bash(cargo bench:*)",
      "Bash(npx playwright install:*)",
      "Bash(./run-benchmark.sh:*)",
      "Bash(rustup run nightly cargo check:*)",
      "Bash(rustup install:*)",
      "Bash(rustup run nightly-2025-07-14 cargo check:*)",
      "Bash(rustup run nightly-2025-07-14 cargo doc:*)",
      "Bash(cargo +nightly-2025-07-14 build:*)",
      "Bash(cargo +nightly-2025-07-14 test:*)",
      "Bash(rustup toolchain:*)",
      "Bash(rustup override set:*)",
      "Bash(RUST_BACKTRACE=1 cargo +nightly test:*)",
      "Bash(rustup run nightly-2025-01-02 cargo test:*)",
      "Bash(rustup run nightly-2025-07-01 cargo test:*)"
    ]
  },
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "\"$CLAUDE_PROJECT_DIR\"/.claude/hooks/session-start.sh"
          }
        ]
      },
      {
        "hooks": [
          {
            "type": "command",
            "command": "\"$CLAUDE_PROJECT_DIR\"/.claude/hooks/post-compact-reload.sh"
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "\"$CLAUDE_PROJECT_DIR\"/.claude/hooks/pre-compact-save.sh"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Read",
        "hooks": [
          {
            "type": "command",
            "command": "\"$CLAUDE_PROJECT_DIR\"/.claude/hooks/capture-project-context.sh"
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "\"$CLAUDE_PROJECT_DIR\"/.claude/hooks/pre-commit.sh \"$TOOL_INPUT\""
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "$CLAUDE_PROJECT_DIR/.claude/statusline.sh"
  },
  "alwaysThinkingEnabled": true
}
