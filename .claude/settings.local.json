      "Bash(docker image inspect:*)",
      "Bash(timeout:*)",
      "Bash(.claude/docker/run-autonomous.sh:*)",
      "Bash(ls:*)",
      "Bash(find:*)",
      "Bash(grep:*)",
      "Bash(tree:*)",
      "Bash(wc:*)",
      "Bash(du:*)",
      "Bash(file:*)",
      "Bash(shasum:*)",
      "Bash(cat:*)",
      "Bash(tail:*)",
      "Bash(sort:*)",
      "Bash(jq:*)",
      "Bash(lsof:*)",
      "Bash(mkdir -p:*)",
      "Bash(chmod:*)",
      "Bash(date:*)",
      "Bash(curl:*)",
      "Bash(make:*)",
      "Bash(node:*)",
      "Bash(npx playwright test:*)",
      "Bash(timeout 120 npx playwright test:*)",
      "Bash(brew:*)",
      "Bash(objdump:*)",
      "Bash(llvm-objdump:*)",
      "Bash(riscv64-unknown-elf-objdump:*)",
      "Bash(riscv64-unknown-elf-gcc:*)",
      "Bash(~/.rustup/toolchains/stable-aarch64-apple-darwin/lib/rustlib/aarch64-apple-darwin/bin/llvm-objdump:*)",
      "Bash(rzup:*)",
      "Bash(sp1up --version:*)",
      "Bash(~/.sp1/bin/cargo-prove:*)",
      "Bash(./target/release/allir-cli:*)",
      "Bash(./scripts/patch-sp1.sh:*)",
      "Bash(./build.sh:*)",
      "Bash(./build-web.sh:*)",
      "Bash(xargs cat:*)",
      "Bash(# Check if server is still running with proper headers curl -s -I http://localhost:8080)",
      "Bash(tee:*)",
      "Bash(npm install)",
      "Bash(npx tsc:*)",
      "Bash(npm run dev:*)",
      "Bash(npm test:*)",
      "Bash(npm run test:e2e:*)",
      "Bash(PORT=8085 npx playwright test:*)",
      "Bash(npm run build:*)",
